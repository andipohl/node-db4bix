                    <params>&lt;parms type=&quot;oracle&quot; prefix=&quot;oracle.&quot;&gt;&#13;
&lt;server&gt;&#13;
&lt;discovery time=&quot;60&quot; item=&quot;discovery.DB4bix.config[temp_usage.01,{$DSN}]&quot; names=&quot;TEMP_TS_NAME&quot;&gt;select temp_ts_name from ((select tablespace_name temp_ts_name from dba_temp_files group by tablespace_name) union (select tablespace_name temp_ts_name from (select dt.tablespace_name tablespace_name from dba_data_files df,dba_tablespaces dt where dt.tablespace_name= df.tablespace_name and dt.contents = 'TEMPORARY') group by tablespace_name) union (select 'TEMP_Summary' temp_ts_name from dual)) order by temp_ts_name&lt;/discovery&gt;&#13;
&lt;query time=&quot;60&quot; item=&quot;temp_total_size[%1]&quot; nodata=&quot;0&quot;&gt;select nvl(temp_ts_name,'TEMP_Summary') temp_ts_name, sum(total_bytes) total_bytes from ((select tablespace_name temp_ts_name, sum(bytes) as total_bytes from dba_temp_files group by tablespace_name) union (select tablespace_name temp_ts_name, sum(bytes) as total_bytes from (select dt.tablespace_name tablespace_name, df.bytes from dba_data_files df,dba_tablespaces dt where dt.tablespace_name = df.tablespace_name and dt.contents = 'TEMPORARY') group by tablespace_name)) group by rollup(temp_ts_name) order by temp_ts_name&lt;/query&gt;&#13;
&lt;query time=&quot;60&quot; item=&quot;temp_used_size[%1]&quot; nodata=&quot;0&quot;&gt;SELECT nvl(A.tablespace_name,'TEMP_Summary') temp_ts_name, SUM(A.used_blocks*D.block_size) used_bytes FROM v$sort_segment A, (SELECT B.name, C.block_size FROM v$tablespace B, v$tempfile C WHERE B.ts#= C.ts# GROUP BY B.name, C.block_size) D WHERE A.tablespace_name= D.name GROUP by rollup(A.tablespace_name) order by temp_ts_name&lt;/query&gt;&#13;
&lt;/server&gt;&#13;
&lt;/parms&gt;</params>
